{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "event": {
      "const": "PerformanceAppStartup"
    },
    "properties": {
      "allOf": [
        {
          "$ref": "common-properties#"
        }
      ],
      "type": "object",
      "required": ["start_time", "destination"],
      "properties": {
        "start_time": {
          "type": "number",
          "minimum": 0
        },
        "destination": {
          "type": "string",
          "format": "any-string"
        }
      },
      "unevaluatedProperties": false
    }
  },
  "unevaluatedProperties": false,
  "required": ["event", "properties"]
}

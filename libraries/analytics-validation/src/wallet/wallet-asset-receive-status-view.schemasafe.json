{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "event": {
      "const": "WalletAssetReceiveStatusView"
    },
    "properties": {
      "allOf": [
        {
          "$ref": "common-properties#"
        }
      ],
      "type": "object",
      "required": ["asset_name", "network"],
      "properties": {
        "asset_name": {
          "type": "string"
        },
        "network": {
          "type": "string"
        }
      },
      "unevaluatedProperties": false
    }
  },
  "unevaluatedProperties": false,
  "required": ["event", "properties"]
}

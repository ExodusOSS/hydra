{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "event": {
      "const": "WalletStyleCardsDonutToggle"
    },
    "properties": {
      "allOf": [
        {
          "$ref": "common-properties#"
        }
      ],
      "type": "object",
      "required": ["mode_activated"],
      "properties": {
        "mode_activated": {
          "type": "string",
          "enum": ["donut", "card"]
        }
      },
      "unevaluatedProperties": false
    }
  },
  "unevaluatedProperties": false,
  "required": ["event", "properties"]
}
